    "module": "es6",
    "baseUrl": "./src"
    // "paths": {
    //   "@/api/*": ["src/api/*"],
    //   "@/components/*": ["./src/components/*"],
    //   "@/styledComponents/*": ["./src/components/StyledComponents/*"],
