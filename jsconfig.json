{
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@components/*": ["./src/components/*"],
      "@pages/*": ["./src/pages/*"],
      "@api/*": ["./src/api/*"],
      "@constants/*": ["./src/constants/*"],
      "@routes/*": ["./src/routes/*"],
      "@redux/*": ["./src/store/redux/*"],
      "@context/*": ["./src/store/context/*"],
      "@customHooks/*": ["./src/store/hooks/custom/*"],
      "@apiHooks/*": ["./src/store/hooks/api/*"],
      "@theme/*": ["./src/theme/*"],
      "@utils/*": ["./src/utils/*"]
    }
  },
  "include": ["src"]
}